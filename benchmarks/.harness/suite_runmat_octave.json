{
  "version": 1,
  "global": {
    "iterations": 3,
    "device_policy": {
      "runmat": "wgpu"
    },
    "timeout_s": 900,
    "fairness": {
      "mode": "perf"
    }
  },
  "cases": [
    {
      "id": "elementwise-math-octave",
      "label": "Elementwise Math (RunMat vs Octave)",
      "case": "elementwise-math",
      "entries": {
        "runmat": "elementwise-math/runmat.m",
        "octave": "elementwise-math/runmat.m"
      },
      "include_impl": [
        "runmat",
        "octave"
      ],
      "params": {
        "defaults": {
          "points": 1000000
        },
        "scale": {
          "key": "points",
          "values": [
            1000000,
            2000000,
            5000000,
            10000000,
            100000000,
            200000000,
            500000000,
            1000000000
          ]
        }
      },
      "env_map": {
        "points": "ELM_POINTS"
      },
      "harness": {
        "warmup": 0,
        "timeout_s": 300
      },
      "plot": {
        "log_x": true,
        "log_x_min": 1000000,
        "speedup_baseline": "octave"
      }
    }
  ]
}

